{
  "name": "trrack-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev:all": "turbo run dev --parallel",
    "dev:rtk": "turbo run dev --parallel --filter=./packages/* --filter=rtk-trrack-example",
    "lint": "turbo run lint",
    "test": "turbo run test --parallel --filter=./packages/*",
    "test:watch": "turbo run test --parallel --filter=./packages/* -- --watch",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "turbo": "^1.2.11"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.18",
  "resolutions": {
    "**/typescript": "^4.6.4",
    "**/@typescript-eslint/eslint-plugin": "^5.26.0",
    "**/@typescript-eslint/parser": "^5.26.0"
  }
}
