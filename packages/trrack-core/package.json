{
  "name": "@visdesignlab/trrack",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.2.0",
  "author": "Kiran Gadhave <kirangadhave2@gmail.com>",
  "description": "",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/visdesignlab/trrack.git"
  },
  "main": "dist/index.js",
  "module": "dist/trrack_dev.esm.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --coverage",
    "test:watch": "tsdx test --coverage --verbose --watch",
    "docs": "typedoc --out docs --target es6 --mode file src",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!src/**/index.{js,ts}"
    ]
  },
  "size-limit": [
    {
      "path": "dist/trrack_dev.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/trrack_dev.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "@types/deep-diff": "^1.0.0",
    "@types/lz-string": "^1.3.33",
    "@types/serialize-javascript": "^4.0.0",
    "size-limit": "^4.6.0",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "deep-diff": "^1.0.2",
    "firebase": "^7.15.5",
    "immer": "^7.0.9",
    "lz-string": "^1.4.4",
    "mobx": "^6.0.1",
    "serialize-javascript": "^5.0.1"
  }
}
